// input: computer is fun ; output: retupmoc si nuf 


import java.util.Scanner;
class reeversewords
{
public static void main(0)
{
Scanner sc = new Scanner (System.in);
System.out.print("enter a seentence :");
String s = sc.nextLine();
s = s +  " ";
String w = " " , ns = " ";
int l=s.length();
for (int i=0;i<l;i++)
{
char ch = s.charAt(i);
if (ch !=' ')
w=ch + w;
else if (ch == ' ')
{
ns = ns +" "+w;
w = " ";
}
}
System.out.println("New string in reverse order by  words ="+ ns);
}
}
